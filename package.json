{
  "name": "@butnothero/axios-wrapper",
  "version": "1.2.0",
  "description": "Generic helper class for wrapping axios calls",
  "repository": {
    "type": "git",
    "url": "https://github.com/butnothero/axios-wrapper"
  },
  "author": "butnothero@gmail.com",
  "license": "MIT",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "./dist/*"
  ],
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "yarn run build:clean && yarn build:js",
    "build:clean": "rimraf dist",
    "build:js": "tsup src/index.ts --format cjs,esm --dts --clean"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@types/lodash": "^4",
    "@types/node": "^18.7.13",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "axios": "^1.6.7",
    "eslint": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "2.4.1",
    "rimraf": "^3.0.2",
    "tsup": "^6.2.2",
    "typescript": "^4.4.4"
  },
  "peerDependencies": {
    "axios": "^1.6.7",
    "lodash": "^4.17.21"
  }
}
